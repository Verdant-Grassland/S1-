package cn.jbit;

import java.awt.Button;
import java.awt.FlowLayout;
import java.awt.Frame;
import java.awt.LayoutManager;

public class CopyOfexample {

	public static void main(String[] args) {
		final Frame f = new Frame("Flowlayout");
		f.setLayout((LayoutManager) new FlowLayout(FlowLayout.LEFT , 20 , 30));
		f.setSize(200 , 300);
		f.add(new Button("第1个按钮"));
		f.add(new Button("第2个按钮"));
		f.add(new Button("第3个按钮"));
		f.add(new Button("第4个按钮"));
		f.add(new Button("第5个按钮"));
		f.add(new Button("第6个按钮"));
		f.add(new Button("第1个按钮"));
		f.add(new Button("第2个按钮"));
		f.add(new Button("第3个按钮"));
		f.add(new Button("第4个按钮"));
		f.add(new Button("第5个按钮"));
		f.add(new Button("第6个按钮"));
		f.add(new Button("第7个按钮"));
		f.add(new Button("第8个按钮"));
		f.add(new Button("第9个按钮"));
		f.add(new Button("第10个按钮"));
		f.add(new Button("第11个按钮"));
		f.add(new Button("第12个按钮"));
		f.add(new Button("第13个按钮"));
		f.add(new Button("第14个按钮"));
		f.add(new Button("第15个按钮"));
		f.add(new Button("第16个按钮"));
		f.add(new Button("第17个按钮"));
		f.add(new Button("第18个按钮"));
		f.setVisible(true);
	}
}
