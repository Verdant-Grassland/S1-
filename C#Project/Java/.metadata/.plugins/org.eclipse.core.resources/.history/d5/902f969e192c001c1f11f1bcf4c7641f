package cn.jbit;

import java.util.Scanner;

public class IF3 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("我想买车我有多少万");
		Scanner input=new Scanner(System.in);
		int yuan=input.nextInt();
		if (yuan>=500){
			System.out.println("就买凯迪拉克");
		if (yuan>=500){
				System.out.println("就买凯迪拉克");
			}
		if (yuan>=100){
				System.out.println("就买帕萨特");
			}
		if (yuan>=500){
				System.out.println("就买凯迪拉克");
			}
		}
			
	}

}
