package cn.jbit;

import java.util.Scanner;

public class TestNumber {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		Number num = new Number();
		System.out.println("输入a的值数:");
		int a = input.nextInt();
		System.out.println("输入b的值数:");
		int b = input.nextInt();
		num.getMax(a, b);
	}
}
