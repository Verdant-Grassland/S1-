package CN.jbit;

import java.util.Scanner;

public class ZY_2 {
	public static void main(String[] args) {
		System.out.println("输入一个数");
		Scanner input=new Scanner(System.in);
		int sum=input.nextInt();
		if (sum/2==0){
			System.out.println("true");
		}else{
			System.out.println("false");
		}
		
		
	}
}