package cn.jbit;

public class InputData {
	public int count(String word){
		int Tum=0;
		int niu[]=new int[2]; 
		for(int i=0;i<word.length();i++){
			if(Tum<='a'&&Tum>='z'||Tum<='A'&&Tum>='Z'){	
				niu[0]++;
				return niu[0];
			}else{
				niu[1]++;
				return niu[1];
			}
		}
		return Tum;
	}
}
