package cn.jbit;

public class ZY5 {
	public static void main(String[] args){
		int liSi=80;    //学员李四的成绩
		boolean isBiq;
		
		Scanner input=new Scanner(System.in);
		System.out.println("输入学员张三的成绩：");
		int ZhangSan=input.nextlnt();
		isBiq=ZhangSan> liSi;
		System.out.println("张三成绩比李四高吗？"+isBiq);
	}
}
