package cn.jbit;

import java.util.Scanner;

public class TestStudent {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		Student student = new Student();
		System.out.println("输入您的学号:");
		student.stuNo = input.nextInt();
		System.out.println("输入您的姓名:");
		student.name = input.next();
		System.out.println("输入您的年龄:");
		student.age = input.nextInt();
		if(student.age >= 7 && student.age <= 13){
			System.out.println("小学");
		}else if(student.age >= 13 && student.age <= 16){
			System.out.println("中学");
		}else if(student.age >= 16 && student.age <= 19){
			System.out.println("中学");
		}else if(student.age >= 19 && student.age <= 23){
			System.out.println("大学");
		}
		System.out.println("输入您的性别:");
		student.sex = input.next();
		return getGrade();
	}
}
