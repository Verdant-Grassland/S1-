package cn.jbit;

import java.util.Scanner;

public class If {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
			Scanner input=new Scanner(System.in);
			int java=input.nextInt();
			System.out.println("输入java的成绩"+java);
			int yin=input.hashCode();
			if((java>80&&yin>80)
				||(java==100&&yin>70));
			System.out.println("老师说奖励你一瓶冰红茶地水");
	}

}
