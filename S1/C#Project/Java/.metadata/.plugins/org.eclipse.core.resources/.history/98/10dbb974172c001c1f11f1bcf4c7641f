package cn.jbit;

import java.util.Scanner;

public class IF2 {
	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
			System.out.println("中了多少万");
			int cai=input.nextInt();
			if(cai>=500){
				System.out.println("如果中了500万我就全买冰红茶地水");
			}else if(cai>=400){
				System.out.println("就去找fw刀");
			}else if(cai>=300){
				System.out.println("就来200个后空翻");
			}
	}
}
