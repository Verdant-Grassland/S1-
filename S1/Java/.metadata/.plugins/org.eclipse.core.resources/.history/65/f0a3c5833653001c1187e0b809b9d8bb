package cn.jbit;

public class Print {
	public void myOutPut(){
		for(int i = 100 ; i <= 999 ; i++){
			int baiwei = i/100%10;
			int shiwei = i/10%10;
			int gewei = i%10;
			int sum = baiwei * baiwei * baiwei + shiwei * shiwei * shiwei + gewei * gewei * gewei;
			if(sum == i){
				System.out.println(sum);
			}
		}
	}
}
