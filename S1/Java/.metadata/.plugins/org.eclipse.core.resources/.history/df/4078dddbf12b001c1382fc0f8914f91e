package CN.jbit;

public class pay {
	public static void main(String[] args){
		//定义
		int shirtPrice=245;
		int sgiePrice=570;
		int padPrice=320;
		
		int shirtNo=2;
		int shoeNo=1;
		int padNo=1;
		
		double discount=0.8;
		//计算价格
		double zho=(shirtPrice*shirtNo+sgiePrice*shoeNo+padPrice*padNo)*discount;
		System.out.println("消费总金额："+zho);
	}
}
