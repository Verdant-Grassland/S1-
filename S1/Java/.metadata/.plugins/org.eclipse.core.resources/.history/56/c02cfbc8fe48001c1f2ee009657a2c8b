package cn.jbit;

import java.util.Scanner;

public class Person {
	String name;
	int age;
	int popularity;
	Scanner input = new Scanner(System.in);
	public String sayHi(){
		
		String name = input.next();
		return name;
	}
	public int nian(){
		int age = input.nextInt();
		return age;
	}
	public int popularity(){
		int popularity = input.nextInt();
		return popularity;
	}
}
