package cn.jbiter;

public class Computer {
	String name = "电脑";
	int scores;
	public void showFist(){
		int random = (int)(Math.random() * 3 + 1);
		switch(random){
		case 1:
			System.out.println("你出拳:剪刀");
			break;
		case 2:
			System.out.println("你出拳:石头");
			break;
		case 3:
			System.out.println("你出拳:布");
			break;
		}
		return random;
	}
}